@model List<Entity.Concrete.Destination>

@{
    ViewBag.Title = "title";
    Layout = "_UserLayout";
}


<!doctype html>
<html lang="zxx">
<body>
  <!--header-->
 
  <!-- //header -->
  <!-- about breadcrumb -->
  <section class="w3l-about-breadcrumb text-left">
    <div class="breadcrumb-bg breadcrumb-bg-about py-sm-5 py-4">
      <div class="container">
        <h2 class="title">Destinasjoner </h2>
        <ul class="breadcrumbs-custom-path mt-2">
          <li><a href="#url">Hjem</a></li>
          <li class="active"><span class="fa fa-arrow-right mx-2" aria-hidden="true"></span> Destinasjoner</li>
        </ul>
      </div>
    </div>
  </section>
  <!-- //about breadcrumb -->
 <!--  Work gallery section -->
 <section class="grids-1 py-5">
  <div class="grids py-lg-5 py-md-4">
      <div class="container">
          <h3 class="hny-title mb-5">Destinasjoner</h3>
          <div class="row">
              @foreach(var item in Model)
              {
              <div class="col-lg-4 col-md-4 col-6">
                  <div class="column">
                      <a href="/Destination/Details/@item.Id"><img src="/@item.ImageUrl" alt="" class="img-fluid"></a>
                      <div class="info">
                          <h4><a href="#">@item.City</a></h4>
                          <p>@item.DayNight</p>
                          <div class="dst-btm">
                              <h6 class="">Starter Fra </h6>
                              <span>NOK @item.Price</span>
                          </div>
                      </div>
                  </div>
              </div>
              }
          </div>
      </div>
</div></section>
<link rel="stylesheet" href="~/Traversal-Liberty/assets/css/style-liberty.css">
<script src="~/Traversal-Liberty/assets/js/bootstrap.min.js"></script>
</body>

</html>